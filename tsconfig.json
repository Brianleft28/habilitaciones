{
    "extends": "./.svelte-kit/tsconfig.json",
    "compilerOptions": {
      "target": "es2017",
      "module": "es2020",
      "strict": true,
      "moduleResolution": "node",
      "sourceMap": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules/*", "__sapper__/*", "public/*"]
  }